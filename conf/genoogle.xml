<?xml version="1.0" encoding="UTF-8"?>

<genoogle:conf xmlns:genoogle="http://genoogle.pih.bio.br">

  <genoogle:search-manager>
    <genoogle:max-simultaneous-searchs value="1"/>     
  </genoogle:search-manager>

  <genoogle:search-parameters>
    <genoogle:max-sub-sequence-distance   value="30"/>
    <genoogle:min-evalue                  value="1E-5" />
    <genoogle:extend-dropoff              value="3" />
    <genoogle:max-hits-results            value="50" />
    <genoogle:max-threads-index-search    value="2" />
    <genoogle:min-query-slice-length      value="250" />
    <genoogle:query-split-quantity        value="1" />
  </genoogle:search-parameters>

  <genoogle:databanks>
  
  	   <!-- 
  <genoogle:split-databanks name="RefSeq_11" path="files/fasta" mask="111010010100110111" number-of-sub-databanks="1" sub-sequence-length="11">
    <genoogle:databank name="Frog"        path="frog.rna.fna"  storage="memory" /> 
    <genoogle:databank name="Human"       path="human.rna.fna" storage="memory" />
    <genoogle:databank name="Mouse"       path="mouse.rna.fna" storage="memory" />
    <genoogle:databank name="Rat"         path="rat.rna.fna" storage="memory" />
    <genoogle:databank name="Zebrafish"   path="zebrafish.rna.fna" storage="memory" />
  </genoogle:split-databanks> 
   -->
  
    <genoogle:split-databanks name="RefSeq_9"  path="files/fasta" mask="1110100110111" number-of-sub-databanks="1" sub-sequence-length="9">
	  <genoogle:databank name="Cow"         path="cow.rna.fna"  storage="memory" />
	  <genoogle:databank name="Frog"        path="frog.rna.fna"  storage="memory" /> 
      <genoogle:databank name="Human"       path="human.rna.fna" storage="memory" />
      <genoogle:databank name="Mouse"       path="mouse.rna.fna" storage="memory" />
      <genoogle:databank name="Rat"         path="rat.rna.fna" storage="memory" />
      <genoogle:databank name="Zebrafish"   path="zebrafish.rna.fna" storage="memory" />
    </genoogle:split-databanks> 
  

  </genoogle:databanks>  	
</genoogle:conf>
